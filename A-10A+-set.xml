<?xml version="1.0"?>
<PropertyList include="A-10-base.xml">
    <sim>
        <variant-id>2</variant-id>
        <variant-of>A-10A</variant-of>
        <description>Fairchild A-10A+ Thunderbolt II (YASim FDM)</description>
        <model>
            <MFD-color> <!--Right now this is just using F-16 colours. We will want to change these at some point-->
                <text1><!--Main text color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </text1>
                <text2><!--BIT/SILENT color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </text2>
                <line1><!--CRM scan lines + lat range + HSD acft, rdr scope color -->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </line1>
                <line2><!--CRM horizon color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </line2>
                <line3><!--CRM DLZ path + cursor + wpt + lock info + data-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </line3>
                <line4><!--CRM EXP color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </line4>
                <line5><!--HSD radius color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </line5>
                <bullseye><!--Bullseye color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </bullseye>
                <betxt><!--Bullseye cursor color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </betxt>
                <lines><!--HSD lines color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </lines>
                <lines2><!--Pull Up cue color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </lines2>
                <circle1><!--HSD red circles + GM Pic color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </circle1>
                <circle2><!--HSD yellow circles + Lock 2 color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </circle2>
                <circle3><!--HSD green circles + CRM IFF + WPN RDY color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </circle3>
                <dot1><!--Lock 1 + cntct blep color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </dot1>
                <dot2><!-- Radar search targets -->
                  <red type="double">0.50</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.50</blue>
                </dot2>
                <dot3><!--Lock 2 color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </dot3>
                <dot4><!--HSD DL cntct color-->
                  <red type="double">0.01</red>
                  <green type="double">1.00</green>
                  <blue type="double">0.07</blue>
                </dot4>
            </MFD-color>
        </model>
    </sim>
    <A-10>
        <stores>
            <tgp-mounted-left type="bool">false</tgp-mounted-left>
            <tgp-mounted-right type="bool">false</tgp-mounted-right>
        </stores>
        <displays>
          <mpcd>
            <holding type="int"></holding>
          </mpcd>
        </displays>
    </A-10>
</PropertyList>