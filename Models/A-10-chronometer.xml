<?xml version="1.0"?>

<PropertyList>

 <name>chronometer</name>
 <path>chronometer.ac</path>

<animation>
  <type>textranslate</type>
  <object-name>h10</object-name>
  <property>sim/time/utc/hour</property>
  <factor>0.01</factor>
  <offset>0</offset>
  <step>10</step>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

