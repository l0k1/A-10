		<property>controls/fuel/tank[4]/boost-pump[0]</property>
		<factor>60</factor>
		<center>
			<x-m>-0.0634</x-m>
			<y-m>-0.0520</y-m>
			<z-m>0.0040</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
