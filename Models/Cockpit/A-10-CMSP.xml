        <property>A-10/avionics/ew-jett-switch</property>
        <factor>60</factor>
        <offset-deg>-30</offset-deg>
        <drag-direction>vertical</drag-direction>
        <center>
            <x-m>-0.001368</x-m>
            <y-m>0.038757</y-m>
            <z-m>0.006</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
