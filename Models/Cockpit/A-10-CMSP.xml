        <factor>60</factor>
        <offset-deg>-30</offset-deg>
        <drag-direction>vertical</drag-direction>
        <center>
            <x-m>0.030643</x-m><y-m>-0.044427</y-m><z-m>0.006</z-m>
        </center>
        <axis>
            <x>0</x><y>-1</y><z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>A-10/avionics/ew-mws-switch</property>
