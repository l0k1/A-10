        <center>
            <x-m>0.030644</x-m><y-m>-0.003501</y-m><z-m>0.006</z-m>
        </center>
        <axis>
            <x>0</x><y>-1</y><z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>A-10/avionics/ew-rwr-switch</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

