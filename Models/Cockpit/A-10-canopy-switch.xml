<?xml version="1.0"?>


<PropertyList>

	<path>A-10-canopy-switch.ac</path>

	<animation>
		<object-name>switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/canopy/canopy-switch</property>
		<factor>30</factor>
		<center>
			<x-m>2.4829</x-m>
			<y-m>0.4717</y-m>
			<z-m>0.8888</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>


	<!-- lighting -->

	<animation>
		<object-name>canopy-switch</object-name>
		<type>material</type>
		<condition>
			<not>
				<property>sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>