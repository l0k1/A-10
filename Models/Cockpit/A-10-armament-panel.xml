<?xml version="1.0"?>

<PropertyList>

	<name>armament-panel</name>
	<path>A-10-armament-panel.ac</path>
	
	<animation>
		<object-name>master-arm-switch</object-name>
		<type>knob</type>
		<property>controls/armament/master-arm-switch</property>
		<factor>30</factor>
		<drag-direction>vertical</drag-direction>
		<center>
			<x-m>0.0029</x-m>
			<y-m>0.0948</y-m>
			<z-m>0.0575</z-m>
		</center>
		<axis><y>-1</y></axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/master-arm-switch</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
	</animation>

	<animation>
		<object-name>gun-rate-switch</object-name>
		<type>knob</type>
		<property>A-10/stations/station[11]/selected</property>
		<factor>30</factor>
		<drag-direction>vertical</drag-direction>
		<center>
			<x-m>0.0031</x-m>
			<y-m>-0.0929</y-m>
			<z-m>0.0575</z-m>
		</center>
		<axis><y>-1</y></axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>A-10/stations/station[11]/selected</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
	</animation>

	<animation>
		<object-name>aim-9-knob</object-name>
		<type>knob</type>
		<property>controls/armament/aim9-knob</property>
		<factor>45</factor>
		<center>
			<x-m>0.0031</x-m>
			<y-m>-0.0535</y-m>
			<z-m>-0.0628</z-m>
		</center>
		<axis><x>-1</x></axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/aim9-knob</property>
                <factor>1</factor>
                <min>0</min>
                <max>2</max>
                <wrap>false</wrap>
            </binding>
        </action>
	</animation>

	<animation>
		<type>select</type>
		<object-name>aim-9-light-on</object-name>
		<condition>
			<property>A-10/stores/aim9-light</property>
		</condition>
	</animation>


	<!--stations-->
	<!--station[0] only AN-ALQ-131 (error on select)-->
	<animation>
		<type>select</type>
		<object-name>s-0-0</object-name>
		<condition>
			<property>controls/armament/master-arm</property>
			<greater-than>
				<property>payload/armament/station/id-0-count</property>
				<value>0</value>
			</greater-than>
			<property>A-10/stations/station[0]/selected</property>
			<not>
				<property>A-10/stores/ecm-mounted-left</property>
				<value>true</value>
			</not>
			<greater-than>
				<property>systems/electrical/R-AC-volts</property>
