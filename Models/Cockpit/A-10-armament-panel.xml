<?xml version="1.0"?>

<PropertyList>

	<name>armament-panel</name>
	<path>A-10-armament-panel.ac</path>

	<animation>
		<object-name>master-arm-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/weapons/master-arm-switch</property>
		<factor>30</factor>
		<center>
			<x-m>0.0029</x-m>
			<y-m>0.0948</y-m>
			<z-m>0.0575</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<object-name>gun-rate-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/weapons/gun-rate-switch</property>
		<factor>30</factor>
		<center>
			<x-m>0.0031</x-m>
			<y-m>-0.0929</y-m>
			<z-m>0.0575</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<object-name>aim-9-knob</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/weapons/dual-AIM-9/aim9-knob</property>
		<factor>45</factor>
		<center>
			<x-m>0.0031</x-m>
			<y-m>-0.0535</y-m>
			<z-m>-0.0628</z-m>
		</center>
		<axis>
			<x>-1</x>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<object-name>aim-9-light-on</object-name>
		<condition>
			<equals>
				<property>sim/model/A-10/weapons/aim-9/cooled</property>
				<value>2</value>
			</equals>
		</condition>
	</animation>


