<?xml version="1.0"?>



<PropertyList>
	<path>A-10-an-arc-186.ac</path>

	<animation>
		<type>select</type>
		<object-name>25</object-name>
		<object-name>50</object-name>
		<object-name>75</object-name>
		<condition>
			<property>sim/model/A-10/dummy-node</property>
		</condition>
	</animation>

<!-- lighting-->

	<effect>
		<inherits-from>Aircraft/A-10/Models/Effects/interior/panel</inherits-from>
		<parameters>
			<implicit-lightmap-threshold-low type="float">0.2</implicit-lightmap-threshold-low>
			<implicit-lightmap-threshold-high type="float">1.2</implicit-lightmap-threshold-high>
			<lightmap-factor n="0"><use>controls/lighting/panel-int</use></lightmap-factor>
		</parameters>
		<object-name>mount.001</object-name>
	</effect>

	<animation>
		<object-name>mount</object-name> 
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<object-name>numbers</object-name> 
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.55</red>
			<green>0.24</green>
			<blue>0.10</blue>
		</emission>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
