<?xml version="1.0"?>

<PropertyList>

	<path>A-10-panel-r3.ac</path>

	<animation>
		<object-name>strobe-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/strobe</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-0.0915</x-m>
			<z-m>-0.0033</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>strobe-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/lighting/strobe</property>
			</binding>
		</action>
	</animation>

	<animation>
		<object-name>nav-flash-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/lighting/nav-lights-switch</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-0.0915</x-m>
			<z-m>-0.0033</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>nav-flash-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>electrical.nav_lights_switcher(1)</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>electrical.nav_lights_switcher(-1)</script>
			</binding>
		</action>
	</animation>

	<animation>
		<object-name>console-adjustable</object-name>
		<type>rotate</type>
		<property>controls/lighting/panel-norm</property>
		<factor>274</factor>
		<center>
			<x-m>0.0326</x-m>
			<y-m>0.0372</y-m>
		</center>
		<axis>
			<z>-1</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>console-adjustable</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/panel-norm</property>
				<step>0.1</step>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/panel-norm</property>
				<step>-0.1</step>
			</binding>
		</action>
	</animation>


	<animation>
		<object-name>fly-inst-knob</object-name>
		<type>rotate</type>
		<property>controls/lighting/instruments-norm</property>
		<factor>274</factor>
		<center>
			<x-m>-0.0070</x-m>
		</center>
		<axis>
			<z>-1</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>fly-inst-knob</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/instruments-norm</property>
				<step>0.1</step>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/instruments-norm</property>
				<step>-0.1</step>
			</binding>
		</action>
	</animation>

	<animation>
		<object-name>formation-knob</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/lighting/formation</property>
		<factor>274</factor>
		<center>
			<x-m>-0.0920</x-m>
		</center>
		<axis>
			<z>-1</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>formation-knob</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/A-10/controls/lighting/formation</property>
				<step>0.1</step>
				<max>1.0</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/A-10/controls/lighting/formation</property>
				<step>-0.1</step>
				<min>0.0</min>
			</binding>
		</action>
	</animation>


	<animation>
		<object-name>mount</object-name>
		<object-name>controls</object-name>
		<object-name>console-adjustable</object-name>
		<object-name>eng-inst-adjustable</object-name>
		<object-name>aux-inst-adjustable</object-name>
		<object-name>flood-adjustable</object-name>
		<type>material</type>
		<condition>
			<not>
				<property>sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>