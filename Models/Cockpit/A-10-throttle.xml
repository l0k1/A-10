<?xml version="1.0"?>


<PropertyList>

	<path>A-10-throttle.ac</path>

	<!-- APU switch -->
	<animation>
		<object-name>apu-start-switch</object-name>
		<type>rotate</type>
		<property>controls/APU/off-start-switch</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0245</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>
	<!-- L engine operation switch -->
	<animation>
		<object-name>l-eng-oper-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/engines/engine[0]/starter-switch-position</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-0.0190</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>
	<!-- R engine operation switch -->
	<animation>
		<object-name>r-eng-oper-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/engines/engine[1]/starter-switch-position</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-0.0190</x-m>
			<z-m>-0.0031</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>


	<!-- Throttles -->
	<animation>
		<object-name>r-throttle</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/engines/engine[1]/throttle</property>
		<factor>41</factor>
		<center>
			<x-m>0.0000</x-m>
			<z-m>-0.0852</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<object-name>l-throttle</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/engines/engine[0]/throttle</property>
		<factor>41</factor>
		<center>
			<x-m>0.0000</x-m>
			<z-m>-0.0852</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>


	<!-- Flaps lever -->
	<animation>
		<object-name>flaps-lever</object-name>
		<type>rotate</type>
		<property>sim/flaps/current-setting</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>-30</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.067</x-m>
			<z-m>-0.008</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<!-- lighting -->

	<animation>
		<object-name>panel</object-name>
		<type>material</type>
		<condition>
			<not>
				<property>sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<object-name>throttles</object-name>
		<type>material</type>
		<condition>
			<not>
				<property>sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.275</red>
			<green>0.12</green>
			<blue>0.05</blue>
		</emission>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>