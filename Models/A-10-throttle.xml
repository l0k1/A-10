<?xml version="1.0"?>


<PropertyList>

  <path>A-10-throttle.ac</path>

<!-- APU switch -->
  <animation>
    <object-name>apu-start-switch</object-name>
    <type>rotate</type>
    <property>sim/model/A-10/controls/APU/off-start-switch</property>
    <interpolation>
      <entry><ind>0</ind><dep>-30</dep></entry>
      <entry><ind>1</ind><dep>30</dep></entry>
    </interpolation>
    <center>
      <x-m>0.0245</x-m>
      <y-m>0.0391</y-m>
      <z-m>-0.0021</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
<!-- L engine operation switch -->
  <animation>
    <object-name>l-eng-oper-switch</object-name>
    <type>rotate</type>
    <property>sim/model/A-10/controls/engines/engine[0]/starter-switch-position</property>
    <interpolation>
      <entry><ind>0</ind><dep>-30</dep></entry>
      <entry><ind>1</ind><dep>0</dep></entry>
      <entry><ind>2</ind><dep>30</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.0190</x-m>
      <y-m>0.0000</y-m>
      <z-m>-0.0021</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
<!-- R engine operation switch -->
  <animation>
    <object-name>r-eng-oper-switch</object-name>
