     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>N1 display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
