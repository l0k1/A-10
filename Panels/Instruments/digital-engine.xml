<?xml version="1.0"?>

<!--
-->

<PropertyList>

 <name>Digital Engine monitor display</name>
 <w-base>40</w-base>
 <h-base>10</h-base>

 <params>
  <eng-num>/engines/engine[0]/engine-num</eng-num>
  <pt-select>/engines/engine[0]/prop-thrust</pt-select>
  <n1-select>/engines/engine[0]/n1</n1-select>
  <n2-select>/engines/engine[0]/n2</n2-select>
  <ff-select>/engines/engine[0]/fuel-flow-gph</ff-select>
  <th-select>/controls/engines/engine[0]/throttle</th-select>
 </params>

 <layers>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/A-10/Panels/Instruments/Textures/trans-orange-bg.png</path>
   </texture>
    <w>380</w>
    <h>40</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>190</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>Eng: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>5</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Number</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/eng-num"/>
     <format>%01.0f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>50</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>PT-Title</name>
   <type>text</type>
   <point-size>18</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PT: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>70</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Prop Thrust display</name>
   <type>text</type>
   <font>led</font>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/pt-select"/>
     <format>%05.f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>105</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>N1-Title</name>
   <type>text</type>
   <point-size>18</point-size>
